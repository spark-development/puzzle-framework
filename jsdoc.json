{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./gen-docs/",
    "encoding": "utf8",
    "recurse": true,
    "private": true,
    "package": "./package.json",
    "readme": "./README.md",
    "template": "./node_modules/ink-docstrap/template"
  }
}
