{
  "name": "puzzle-framework",
  "version": "1.3.2",
  "description": "A modular framework for micro applications/services.",
  "main": "src/server.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --exit tests/**/*.test.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src",
    "bamboo-lint": "./node_modules/.bin/eslint ./src -f node_modules/eslint-bamboo-formatter/reporter.js",
    "jsdoc": "./node_modules/.bin/jsdoc -p -c ./jsdoc.json ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spark-development/puzzle-framework.git"
  },
  "keywords": [
    "puzzle",
    "framework",
    "micro-apps",
    "micro-services",
    "modular framework",
    "http",
    "web"
  ],
  "author": "Valentin Duricu <valentin.duricu@spark-dev.ro>",
  "bugs": "https://github.com/spark-development/puzzle-framework/issues",
  "license": "MIT",
  "homepage": "https://puzzle.spark-dev.ro/",
  "dependencies": {
    "express": "^4.16.3",
    "joi": "^13.6.0",
    "joi-i18n": "^13.1.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.1",
    "puzzle-framework-lite": "latest",
    "sequelize": "^4.38.1",
    "umzug": "^2.1.0",
    "verbal-expressions": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "debug": "^4.0.1",
    "eslint": "^5.6.0",
    "eslint-bamboo-formatter": "0.1.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "ink-docstrap": "^1.3.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-bamboo-reporter": "^1.1.1"
  },
  "files": [
    "src",
    "dist"
  ],
  "directories": {
    "doc": "docs",
    "example": "sample",
    "test": "tests"
  }
